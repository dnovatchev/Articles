    "$gen2ToInf?take(5)?filter(fn($n){$n ge 7})?some()",
     $gen2ToInf?take(5)?filter(fn($n){$n ge 7})?some(),  
     "$gen2ToInf?take(5)?some-where(fn($n){$n ge 7})",
     $gen2ToInf?take(5)?some-where(fn($n){$n ge 7}), 
