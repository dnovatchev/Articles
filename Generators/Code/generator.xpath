    "================",
    "==>  $gen2ToInf?filter(fn($n){$n mod 2 eq 1})?get-current()",
    $gen2ToInf?filter(fn($n){$n mod 2 eq 1})?get-current(),
    "$gen2ToInf?filter(fn($n){$n mod 2 eq 1})?move-next()?get-current()",
    $gen2ToInf?filter(fn($n){$n mod 2 eq 1})?move-next()?get-current(),
    "================", 
    "$gen2ToInf?filter(fn($n){$n mod 2 eq 1})?take(10)?to-array()",
